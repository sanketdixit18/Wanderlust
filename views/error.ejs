<% layout('layouts/boilerplate') %>

<div class="row mt-3">
<div class="col-6 offset-3">
<div class="alert alert-danger" role="alert">
<h4 class="alert-heading">Error (<%= err.statusCode %>)</h4>
<p><%= err.message %></p>

        <% if (process.env.NODE_ENV !== "production") { %>
        <hr>
        <p class="mb-0"><%= err.stack %></p>
        <% } %>
    </div>
    <div class="text-center">
        <a href="/listings" class="btn btn-outline-secondary">Go to Listings</a>
    </div>
</div>

</div>